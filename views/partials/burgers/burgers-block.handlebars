<li class="burgerList z-depth-2">
    {{ burger_name }}

    {{#unless devoured}}<button class="waves-effect waves-light light-green btn z-depth-2 devourButton" data-id="{{id}}" data-newdevoured="{{devoured}}"> DEVOUR </button>{{/unless}}

    {{#if devoured}}<button class="waves-effect waves-light light-green btn z-depth-2 devourButton deleteButton" data-id="{{this.id}}">DELETE</button>{{/if}}

</li>